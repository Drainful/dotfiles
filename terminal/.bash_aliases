alias e="emacsclient -c"
alias edw="emacs-daemon-workspace"
alias nixconf="emacsclient --create-frame -e '(find-file \"/sudo::/etc/nixos/configuration.nix\")'"
alias md="mkdir"
alias ~="cd ~"
alias ..="cd .."
alias ...="cd ../.."
alias spdep="spark deploy ~/.dotfiles/dotfiles.sus"
alias nrs="sudo nixos-rebuild switch"
alias nget="sudo nix-env -i "
alias qlrun="rlwrap quicklisp run"
alias ns="nix-shell"
alias ":q"="exit"
