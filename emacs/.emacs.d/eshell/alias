alias nil  $*
alias ll ls -l $*
alias md mkdir $*
alias ... ../..
alias .... ../../..
alias nrs sudo nixos-rebuild switch
alias nrb sudo nixos-rebuild boot
alias nfind nix-env -f '<nixpkgs>' -qaP $*
alias nquery nix-env --query $*
alias nuninstall sudo nix-env -f '<nixpkgs>' --uninstall $*
alias nget sudo nix-env -f '<nixpkgs>' --install -A $*
alias :q exit
alias def bash ~/auto/def/def.bash
alias tb nc termbin.com 9999
alias clear clear t
alias reconf sudo guix system reconfigure -L $GUIX_CONFIG_DIRECTORY $GUIX_OS_CONFIG_FILE  $*
alias sdo /run/setuid-programs/sudo $*
